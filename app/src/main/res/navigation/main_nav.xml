<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/homepageFragment">

    <fragment
        android:id="@+id/homepageFragment"
        android:name="com.griding.pentaapsida.HomepageFragment"
        android:label="fragment_homepage"
        tools:layout="@layout/fragment_homepage" >
        <action
            android:id="@+id/action_homepageFragment_to_meditasiFragment"
            app:destination="@id/meditasiFragment" />
        <action
            android:id="@+id/action_homepageFragment_to_kooperatifFragment"
            app:destination="@id/kooperatifFragment" />
        <action
            android:id="@+id/action_homepageFragment_to_berdiskusiFragment"
            app:destination="@id/berdiskusiFragment" />
        <action
            android:id="@+id/action_homepageFragment_to_listMotivasiFragment"
            app:destination="@id/listMotivasiFragment" />
        <action
            android:id="@+id/action_homepageFragment_to_olahragaFragment"
            app:destination="@id/olahragaFragment" />
        <action
            android:id="@+id/action_homepageFragment_to_belajarFragment"
            app:destination="@id/belajarFragment" />
    </fragment>
    <fragment
        android:id="@+id/meditasiFragment"
        android:name="com.griding.pentaapsida.MeditasiFragment"
        android:label="fragment_meditasi"
        tools:layout="@layout/fragment_meditasi" >
        <action
            android:id="@+id/action_meditasiFragment_to_manfaatMeditasiFragment"
            app:destination="@id/manfaatMeditasiFragment" />
        <action
            android:id="@+id/action_meditasiFragment_to_praktikMeditasiFragment"
            app:destination="@id/praktikMeditasiFragment" />
        <action
            android:id="@+id/action_meditasiFragment_to_stopwatchFragment"
            app:destination="@id/stopwatchFragment" />
    </fragment>
    <fragment
        android:id="@+id/manfaatMeditasiFragment"
        android:name="com.griding.pentaapsida.ManfaatMeditasiFragment"
        android:label="fragment_manfaat_meditasi"
        tools:layout="@layout/fragment_manfaat_meditasi" >
        <action
            android:id="@+id/action_manfaatMeditasiFragment_to_kesadaranFragment"
            app:destination="@id/kesadaranFragment" />
        <action
            android:id="@+id/action_manfaatMeditasiFragment_to_konsetasiFragment"
            app:destination="@id/konsetasiFragment" />
    </fragment>
    <fragment
        android:id="@+id/praktikMeditasiFragment"
        android:name="com.griding.pentaapsida.PraktikMeditasiFragment"
        android:label="fragment_praktik_meditasi"
        tools:layout="@layout/fragment_praktik_meditasi" />
    <fragment
        android:id="@+id/konsetasiFragment"
        android:name="com.griding.pentaapsida.KonsetasiFragment"
        android:label="fragment_konsetasi"
        tools:layout="@layout/fragment_konsetasi" />
    <fragment
        android:id="@+id/kesadaranFragment"
        android:name="com.griding.pentaapsida.KesadaranFragment"
        android:label="fragment_kesadaran"
        tools:layout="@layout/fragment_kesadaran" />
    <fragment
        android:id="@+id/kooperatifFragment"
        android:name="com.griding.pentaapsida.KooperatifFragment"
        android:label="fragment_kooperatif"
        tools:layout="@layout/fragment_kooperatif" >
        <action
            android:id="@+id/action_kooperatifFragment_to_formKuesionerFragment"
            app:destination="@id/formKuesionerFragment" />
        <action
            android:id="@+id/action_kooperatifFragment_to_tipsFragment"
            app:destination="@id/tipsFragment" />
    </fragment>
    <fragment
        android:id="@+id/formKuesionerFragment"
        android:name="com.griding.pentaapsida.FormKuesionerFragment"
        android:label="fragment_form_kuesioner"
        tools:layout="@layout/fragment_form_kuesioner" />
    <fragment
        android:id="@+id/tipsFragment"
        android:name="com.griding.pentaapsida.TipsFragment"
        android:label="fragment_tips"
        tools:layout="@layout/fragment_tips" />
    <fragment
        android:id="@+id/berdiskusiFragment"
        android:name="com.griding.pentaapsida.BerdiskusiFragment"
        android:label="fragment_berdiskusi"
        tools:layout="@layout/fragment_berdiskusi" />
    <fragment
        android:id="@+id/listMotivasiFragment"
        android:name="com.griding.pentaapsida.ListMotivasiFragment"
        android:label="fragment_list_motivasi"
        tools:layout="@layout/fragment_list_motivasi" />
    <fragment
        android:id="@+id/stopwatchFragment"
        android:name="com.griding.pentaapsida.StopwatchFragment"
        android:label="fragment_stopwatch"
        tools:layout="@layout/fragment_stopwatch" />
    <fragment
        android:id="@+id/belajarFragment"
        android:name="com.griding.pentaapsida.BelajarFragment"
        android:label="fragment_belajar"
        tools:layout="@layout/fragment_belajar" />
    <fragment
        android:id="@+id/cyclingFragment"
        android:name="com.griding.pentaapsida.CyclingFragment"
        android:label="fragment_cycling"
        tools:layout="@layout/fragment_cycling" >
        <action
            android:id="@+id/action_cyclingFragment_to_timerCyclingFragment"
            app:destination="@id/timerCyclingFragment" />
    </fragment>
    <fragment
        android:id="@+id/jogingFragment"
        android:name="com.griding.pentaapsida.JogingFragment"
        android:label="fragment_joging"
        tools:layout="@layout/fragment_joging" >
        <action
            android:id="@+id/action_jogingFragment_to_TImerJogingFragment"
            app:destination="@id/TImerJogingFragment" />
    </fragment>
    <fragment
        android:id="@+id/olahragaFragment"
        android:name="com.griding.pentaapsida.OlahragaFragment"
        android:label="fragment_olahraga"
        tools:layout="@layout/fragment_olahraga" >
        <action
            android:id="@+id/action_olahragaFragment_to_jogingFragment"
            app:destination="@id/jogingFragment" />
        <action
            android:id="@+id/action_olahragaFragment_to_cyclingFragment"
            app:destination="@id/cyclingFragment" />
    </fragment>
    <fragment
        android:id="@+id/timerCyclingFragment"
        android:name="com.griding.pentaapsida.TimerCyclingFragment"
        android:label="fragment_timer_cycling"
        tools:layout="@layout/fragment_timer_cycling" />
    <fragment
        android:id="@+id/TImerJogingFragment"
        android:name="com.griding.pentaapsida.TImerJogingFragment"
        android:label="fragment_t_imer_joging"
        tools:layout="@layout/fragment_t_imer_joging" />
</navigation>